<svg width="180" height="180" viewBox="0 0 180 180" fill="none" xmlns="http://www.w3.org/2000/svg">
  <!-- Background -->
  <rect width="180" height="180" rx="22" fill="url(#bgGradient)"/>

  <!-- Outer glow ring -->
  <circle cx="90" cy="90" r="70" stroke="url(#glowGradient)" stroke-width="1" fill="none" opacity="0.3"/>

  <!-- Main geometric design -->
  <g transform="translate(90, 90)">
    <!-- Central hexagon -->
    <polygon points="-14,-24.25 14,-24.25 28,0 14,24.25 -14,24.25 -28,0"
             fill="url(#centerGradient)"
             stroke="url(#strokeGradient)"
             stroke-width="1.5"/>

    <!-- Inner core -->
    <circle cx="0" cy="0" r="9" fill="url(#coreGradient)"/>

    <!-- Floating elements - representing code/data -->
    <g opacity="0.8">
      <!-- Top elements -->
      <rect x="-3" y="-42" width="6" height="6" rx="1" fill="#14b8a6" opacity="0.6"/>
      <rect x="21" y="-32" width="4" height="4" rx="1" fill="#8b5cf6" opacity="0.7"/>
      <rect x="-25" y="-32" width="4" height="4" rx="1" fill="#06b6d4" opacity="0.5"/>

      <!-- Side elements -->
      <rect x="39" y="-3" width="6" height="6" rx="1" fill="#14b8a6" opacity="0.6"/>
      <rect x="-45" y="-3" width="6" height="6" rx="1" fill="#8b5cf6" opacity="0.7"/>

      <!-- Bottom elements -->
      <rect x="-3" y="36" width="6" height="6" rx="1" fill="#06b6d4" opacity="0.6"/>
      <rect x="21" y="26" width="4" height="4" rx="1" fill="#14b8a6" opacity="0.5"/>
      <rect x="-25" y="26" width="4" height="4" rx="1" fill="#8b5cf6" opacity="0.6"/>
    </g>

    <!-- Connection lines -->
    <g stroke="url(#lineGradient)" stroke-width="0.8" opacity="0.4">
      <line x1="0" y1="-9" x2="0" y2="-36"/>
      <line x1="8" y1="-4" x2="21" y2="-28"/>
      <line x1="-8" y1="-4" x2="-21" y2="-28"/>
      <line x1="9" y1="0" x2="33" y2="0"/>
      <line x1="-9" y1="0" x2="-39" y2="0"/>
      <line x1="0" y1="9" x2="0" y2="30"/>
      <line x1="8" y1="4" x2="21" y2="22"/>
      <line x1="-8" y1="4" x2="-21" y2="22"/>
    </g>
  </g>

  <!-- Brand text -->
  <text x="90" y="150"
        font-family="'Orbitron', monospace"
        font-size="13"
        font-weight="700"
        text-anchor="middle"
        fill="url(#textGradient)">
    OneKnight
  </text>

  <!-- Subtle accent line under text -->
  <line x1="63" y1="158" x2="117" y2="158"
        stroke="url(#accentGradient)"
        stroke-width="1"
        opacity="0.6"/>

  <!-- Gradient Definitions -->
  <defs>
    <!-- Background gradient -->
    <linearGradient id="bgGradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#0f0f0f;stop-opacity:1" />
      <stop offset="50%" style="stop-color:#1a1a1a;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#0a0a0a;stop-opacity:1" />
    </linearGradient>

    <!-- Glow gradient -->
    <linearGradient id="glowGradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#14b8a6;stop-opacity:0.8" />
      <stop offset="50%" style="stop-color:#8b5cf6;stop-opacity:0.6" />
      <stop offset="100%" style="stop-color:#06b6d4;stop-opacity:0.8" />
    </linearGradient>

    <!-- Center gradient -->
    <linearGradient id="centerGradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#14b8a6;stop-opacity:0.15" />
      <stop offset="50%" style="stop-color:#8b5cf6;stop-opacity:0.2" />
      <stop offset="100%" style="stop-color:#06b6d4;stop-opacity:0.15" />
    </linearGradient>

    <!-- Core gradient -->
    <radialGradient id="coreGradient" cx="50%" cy="50%" r="50%">
      <stop offset="0%" style="stop-color:#ffffff;stop-opacity:0.9" />
      <stop offset="70%" style="stop-color:#14b8a6;stop-opacity:0.8" />
      <stop offset="100%" style="stop-color:#8b5cf6;stop-opacity:0.6" />
    </radialGradient>

    <!-- Stroke gradient -->
    <linearGradient id="strokeGradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#14b8a6;stop-opacity:0.8" />
      <stop offset="50%" style="stop-color:#ffffff;stop-opacity:0.9" />
      <stop offset="100%" style="stop-color:#8b5cf6;stop-opacity:0.8" />
    </linearGradient>

    <!-- Line gradient -->
    <linearGradient id="lineGradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#14b8a6;stop-opacity:0.6" />
      <stop offset="100%" style="stop-color:#8b5cf6;stop-opacity:0.6" />
    </linearGradient>

    <!-- Text gradient -->
    <linearGradient id="textGradient" x1="0%" y1="0%" x2="100%" y2="0%">
      <stop offset="0%" style="stop-color:#14b8a6;stop-opacity:1" />
      <stop offset="50%" style="stop-color:#ffffff;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#8b5cf6;stop-opacity:1" />
    </linearGradient>

    <!-- Accent gradient -->
    <linearGradient id="accentGradient" x1="0%" y1="0%" x2="100%" y2="0%">
      <stop offset="0%" style="stop-color:#14b8a6;stop-opacity:0.8" />
      <stop offset="50%" style="stop-color:#8b5cf6;stop-opacity:0.8" />
      <stop offset="100%" style="stop-color:#06b6d4;stop-opacity:0.8" />
    </linearGradient>
  </defs>
</svg>